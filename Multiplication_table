def multiplication(_n):
    output = []
    for i in range(1, 11):
        output.append(i * _n)
    return output



def table(_begin, _end):
    output = []
    for i in range(_begin, _end):
        output.append(multiplication(i))
    return output
        



var = table(2,8)

for i in range(len(var)):
    print var[i]
